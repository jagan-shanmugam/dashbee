{
  "name": "dashbee",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "clean": "rm -rf .next && rm -rf node_modules/",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:postgres": "playwright test --project=postgres",
    "test:e2e:mysql": "playwright test --project=mysql",
    "test:e2e:sqlite": "playwright test --project=sqlite",
    "test:e2e:all-dbs": "playwright test --project=postgres --project=mysql --project=sqlite",
    "test:llm": "tsx scripts/test-llm.ts",
    "test:all": "pnpm run test && pnpm run test:e2e && pnpm run test:llm",
    "docker:up": "cd sample-db && docker compose up -d",
    "docker:up:postgres": "cd sample-db && docker compose up -d postgres",
    "docker:up:mysql": "cd sample-db && docker compose up -d mysql",
    "docker:down": "cd sample-db && docker compose down",
    "db:seed-sqlite": "tsx scripts/seed-sqlite.ts",
    "db:seed-mysql": "tsx scripts/seed-mysql.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^3.0.13",
    "@ai-sdk/google": "^3.0.31",
    "@ai-sdk/openai": "^3.0.7",
    "@aws-sdk/client-s3": "^3.990.0",
    "@google-cloud/storage": "^7.19.0",
    "@json-render/core": "^0.2.0",
    "@json-render/react": "^0.2.0",
    "@langfuse/otel": "^4.6.1",
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@types/better-sqlite3": "^7.6.13",
    "ai": "^6.0.33",
    "apache-arrow": "^21.1.0",
    "better-sqlite3": "^12.6.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.562.0",
    "maplibre-gl": "^5.16.0",
    "mysql2": "^3.16.2",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "parquet-wasm": "^0.7.1",
    "pg": "^8.11.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "xlsx": "^0.18.5",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.3.0",
    "@next/eslint-plugin-next": "^16.1.6",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.10.0",
    "@types/papaparse": "^5.5.2",
    "@types/pg": "^8.10.0",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "canvas": "^3.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.17"
  }
}
